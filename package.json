{
  "name": "castbot",
  "private": true,
  "version": "1.0.0",
  "description": "Development version of ORG bot that allows you to automate the castlist process to easily create and update an interactive castlist in discord.",
  "main": "app.js",
  "type": "module",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "deploy-commands": "node manage-commands.js",
    "clean-commands": "node manage-commands.js --clean-only",
    "verify-commands": "node manage-commands.js --verify-only",
    "analyze-commands": "node manage-commands.js --analyze-only",
    "deploy-remote": "node deploy-remote.js",
    "deploy-commands-remote": "node deploy-remote.js --commands-only",
    "deploy-remote-dry-run": "node deploy-remote.js --dry-run",
    "logs-remote": "node deploy-remote.js --logs",
    "status-remote": "node deploy-remote.js --status",
    "remote-help": "node deploy-remote.js --help"
  },
  "author": "Shay DeWael",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.9.0",
    "discord-interactions": "^4.0.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "sharp": "^0.33.2",
    "gifsicle": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "nodemon": "^3.0.0",
    "typescript": "^5.7.2",
    "wrangler": "^3.99.0"
  }
}
